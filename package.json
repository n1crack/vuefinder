{
  "name": "vuefinder",
  "version": "4.0.10",
  "description": "A sleek, developer-friendly file manager for Vue — organize, preview, and manage files with ease.",
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vuefinder.js",
      "default": "./dist/vuefinder.js"
    },
    "./dist/vuefinder.css": "./dist/vuefinder.css",
    "./dist/style.css": "./dist/vuefinder.css"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --declaration --emitDeclarationOnly",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "homepage": "https://github.com/n1crack/vuefinder#readme",
  "keywords": [
    "vue",
    "vue3",
    "file-manager",
    "file-explorer",
    "file-browser",
    "file-upload",
    "vue-component",
    "vue-plugin",
    "cloud-storage",
    "media-manager",
    "uploader",
    "finder"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n1crack/vuefinder.git"
  },
  "bugs": {
    "url": "https://github.com/n1crack/vuefinder/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/n1crack"
  },
  "author": {
    "name": "Yusuf Özdemir",
    "email": "yusuf@ozdemir.be"
  },
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.5.22"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": false
    }
  },
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@nanostores/persistent": "^1.1.0",
    "@nanostores/vue": "^1.0.1",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/vue-query": "^5.90.2",
    "@types/overlayscrollbars": "^1.12.5",
    "@uppy/core": "^5.0.2",
    "@uppy/locales": "^5.0.1",
    "@uppy/vue": "^3.1.0",
    "@uppy/xhr-upload": "^5.0.1",
    "@viselect/vanilla": "^3.9.0",
    "@vueuse/core": "^13.9.0",
    "mitt": "^3.0.1",
    "nanostores": "^1.0.1",
    "overlayscrollbars": "^2.12.0",
    "tailwindcss": "^4.1.13",
    "vanilla-lazyload": "^19.1.3",
    "vite-svg-loader": "^5.1.0",
    "vue-advanced-cropper": "^2.8.9",
    "vue-sonner": "^2.0.9"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.6",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^15.3.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.5.1",
    "jiti": "^2.5.1",
    "jsdom": "^27.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "rollup-plugin-copy": "^3.5.0",
    "start-server-and-test": "^2.1.2",
    "typescript": "~5.9.0",
    "vite": "^7.1.7",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitepress": "^2.0.0-alpha.12",
    "vitest": "^3.2.4",
    "vue-github-button": "^3.1.3",
    "vue-tsc": "^3.1.0"
  }
}
